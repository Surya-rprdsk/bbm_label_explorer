{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ailabelassist",
  "version": "0.1.0",
  "identifier": "com.ailabelassist.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "BBM Label Assistant",
        "maximizable": false,
        "minimizable": false,
        "maxWidth": 450,
        "maxHeight": 300,
        "minHeight": 300,
        "minWidth": 450,
        "focus": true,
        "skipTaskbar": false,
        "alwaysOnTop": true,
        "resizable": true,
        "visible": false,
        "decorations": false,
        "transparent": true
      },
      {
        "label": "splashscreen",
        "url": "/index.html#splash",
        "visible": true,
        "title": "",
        "width": 400,
        "height": 200,
        "minimizable": false,
        "maximizable": false,
        "closable": false,
        "decorations": false,
        "resizable": false,
        "center": true
      },
      {
        "label": "settings",
        "title": "Settings",
        "url": "/settings.html",
        "minWidth": 500,
        "minHeight": 400,
        "maxWidth": 500,
        "maxHeight": 400,
        "visible": false,
        "resizable": false,
        "decorations": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]  },  "plugins": {
    "updater": {
      "pubkey": "",      "endpoints": [
        "https://github.com/YOUR_GITHUB_USERNAME/AILabelAssist/bbm_label_explorer/releases/latest/download/latest.json"
      ],
      "dialog": true,
      "active": true
    }
  }
}
